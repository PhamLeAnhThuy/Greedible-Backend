        ],
        "security": [
          {
            "Bearer": []
          }
        ],
        "responses": {
          "200": {
            "description": "User's order history"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/api/payments/create": {
      "post": {
        "summary": "Create a payment",
        "description": "Initiate payment for an order via MoMo or Vietcombank (staff only).",
        "tags": [
          "Payments"
        ],
        "security": [
          {
            "Bearer": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "orderId",
                  "paymentMethod"
                ],
                "properties": {
                  "orderId": {
                    "type": "number"
                  },
                  "paymentMethod": {
                    "type": "string",
                    "enum": [
                      "momo",
                      "vietcombank"
                    ]
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Payment created, returns payment URL"
          },
          "401": {
            "description": "Unauthorized"
          },
          "404": {
            "description": "Order not found"
          },
          "500": {
            "description": "Server error"
          }
        }
      }
    },
    "/api/payments/momo/callback": {
      "post": {
        "summary": "MoMo payment callback",
        "description": "Webhook endpoint for MoMo to send payment status updates.",
        "tags": [
          "Payments"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Callback processed"
          },
          "500": {
            "description": "Server error"
          }
        }
